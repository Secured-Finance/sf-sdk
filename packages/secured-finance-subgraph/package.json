{
  "name": "secured-finance-subgraph",
  "version": "0.1.7-beta.5",
  "private": "true",
  "scripts": {
    "create": "graph create secured-finance/secured-finance-protocol --node https://api.thegraph.com/deploy/",
    "create-local": "graph create secured-finance/secured-finance-protocol --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph codegen && graph build",
    "test": "graph test",
    "coverage": "graph test -- -c",
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './**/*.{js,ts,tsx}' --fix",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "deploy": "graph deploy secured-finance/secured-finance-protocol --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy secured-finance/secured-finance-protocol --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "prepublish": "npm run codegen"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.2",
    "@graphprotocol/graph-ts": "^0.26.0",
    "@shopify/eslint-plugin-assemblyscript": "^0.1.13",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethers": "^5.6.8",
    "matchstick-as": "0.4.3",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
