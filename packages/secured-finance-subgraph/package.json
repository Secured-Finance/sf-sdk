{
  "name": "secured-finance-subgraph",
  "version": "0.1.7-beta.14",
  "private": "true",
  "scripts": {
    "generate:dev": "ts-node ./scripts/generate.ts development",
    "generate:stg": "ts-node ./scripts/generate.ts staging",
    "codegen": "graph codegen ./subgraph.yaml",
    "build": "graph codegen && graph build",
    "test": "graph test",
    "coverage": "graph test -- -c",
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './**/*.{js,ts,tsx}' --fix",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "deploy:local": "graph deploy secured-finance/sf-protocol-local ./subgraph.localhost.yaml --product hosted-service --output-dir ./build/localhost --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy:dev": "graph deploy secured-finance/sf-protocol-dev ./subgraph.development.yaml --product hosted-service --output-dir ./build/development",
    "deploy:stg": "graph deploy secured-finance/sf-protocol-stg ./subgraph.staging.yaml --product hosted-service --output-dir ./build/staging",
    "prepublish": "npm run codegen"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.2",
    "@graphprotocol/graph-ts": "^0.26.0",
    "@secured-finance/smart-contracts": "^0.0.2-beta.0",
    "@shopify/eslint-plugin-assemblyscript": "^0.1.13",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethers": "^5.6.8",
    "js-yaml": "^4.1.0",
    "matchstick-as": "0.4.3",
    "prettier": "2.6.2",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
