{
  "name": "secured-finance-subgraph",
  "version": "0.1.7-beta.9",
  "private": "true",
  "scripts": {
    "migration:dev": "ts-node ./scripts/migration.ts development",
    "migration:stg": "ts-node ./scripts/migration.ts staging",
    "create:local": "graph create secured-finance/sf-protocol-local --node http://127.0.0.1:8020",
    "test": "echo \"graph test\"",
    "coverage": "graph test -- -c",
    "lint": "eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './**/*.{js,ts,tsx}' --fix",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json'",
    "deploy:local": "graph deploy secured-finance/sf-protocol-local ./src/development/subgraph.yaml --product hosted-service --output-dir ./src/development/build --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy:dev": "graph deploy secured-finance/sf-protocol-dev ./src/development/subgraph.yaml --product hosted-service --output-dir ./src/development/build",
    "deploy:stg": "graph deploy secured-finance/sf-protocol-stg ./src/staging/subgraph.yaml --product hosted-service --output-dir ./src/staging/build",
    "prepublish": "npm run codegen"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.2",
    "@graphprotocol/graph-ts": "^0.26.0",
    "@secured-finance/smart-contracts": "github:Secured-Finance/smart-contracts#SF-18-add-staging",
    "@shopify/eslint-plugin-assemblyscript": "^0.1.13",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethers": "^5.6.8",
    "js-yaml": "^4.1.0",
    "matchstick-as": "0.4.3",
    "prettier": "2.6.2",
    "ts-node": "^10.8.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
