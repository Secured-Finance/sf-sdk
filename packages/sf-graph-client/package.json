{
  "name": "@secured-finance/sf-graph-client",
  "version": "0.1.6-beta.14",
  "description": "GraphQL react client for querying Secured Finance subgraph",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint lib/ src/ test/ --ext .ts",
    "build": "npm run compile && tsc -b",
    "test": "ts-mocha --timeout 4000 --delay src/*.test.{js,jsx,ts,tsx} src/**/*.test.{js,jsx,ts,tsx}",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "postinstall": "npm run generate",
    "generate": "npx graphql-codegen"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk/packages/sf-graph-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/sf-sdk"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "codegen-graph-ts": "^0.1.4",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.11",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-operations": "^2.3.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.5",
    "@types/mocha": "^9.1.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.5.4",
    "mocha": "^9.2.0",
    "moment": "^2.29.1",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "wait-for-expect": "^3.0.2",
    "web3-utils": "^1.7.0"
  },
  "types": "dist/index.d.ts"
}
