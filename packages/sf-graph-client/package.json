{
  "name": "@secured-finance/sf-graph-client",
  "version": "0.1.7-beta.21",
  "description": "GraphQL react client for querying Secured Finance subgraph",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "codegen": "npm run codegen:dev && npm run codegen:stg",
    "codegen:dev": "SUBGRAPH_NAME=sf-protocol-dev graphclient build --dir src/graphclients/development",
    "codegen:stg": "SUBGRAPH_NAME=sf-protocol-stg graphclient build --dir src/graphclients/staging",
    "build": "npm run build:dev && npm run build:stg",
    "build:dev": "npm run compile && tsc -b",
    "build:stg": "npm run compile && tsc -b",
    "prebuild": "npm run codegen",
    "test": "ts-mocha --timeout 6000 src/**/*.test.{ts,tsx} test/**/*.test.ts",
    "test:cov": "nyc npm run test"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk/packages/sf-graph-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/sf-sdk"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@graphprotocol/client-apollo": "^1.0.1",
    "@graphprotocol/client-auto-pagination": "^1.0.3",
    "@graphprotocol/client-block-tracking": "^1.0.3",
    "@graphprotocol/client-cli": "^2.0.0",
    "cross-fetch": "^3.1.5",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/mocha": "^9.1.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.3.6",
    "ethers": "^5.5.4",
    "global-jsdom": "^8.4.0",
    "graphql": "^16.4.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.0",
    "moment": "^2.29.1",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "wait-for-expect": "^3.0.2",
    "web3-utils": "^1.7.0"
  },
  "peerDependencies": {
    "graphql": "^16.4.0"
  },
  "types": "dist/index.d.ts",
  "files": ["dist/"]
}
