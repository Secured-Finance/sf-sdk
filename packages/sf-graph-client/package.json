{
  "name": "@secured-finance/sf-graph-client",
  "version": "0.2.0-beta.22",
  "description": "GraphQL react client for querying Secured Finance subgraph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "codegen": "npm run codegen:dev && npm run codegen:stg",
    "codegen:dev": "graphclient build --dir src/graphclients/development",
    "codegen:stg": "graphclient build --dir src/graphclients/staging",
    "build": "npm run build:dev && npm run build:stg",
    "build:dev": "npm run compile && tsc -b",
    "build:stg": "npm run compile && tsc -b",
    "prebuild": "npm run codegen",
    "update-subgraph-version": "ts-node ./scripts/update-subgraph-version.ts",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk/packages/sf-graph-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/sf-sdk"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@graphprotocol/client-apollo": "^1.0.11",
    "@graphprotocol/client-auto-pagination": "^1.1.8",
    "@graphprotocol/client-cli": "^2.2.10",
    "cross-fetch": "^3.1.5",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "^16.14.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "babel-preset-react": "^6.24.1",
    "commander": "^9.4.1",
    "ethers": "^5.7.1",
    "global-jsdom": "^8.4.0",
    "graphql": "^16.6.0",
    "jest": "^29.1.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "wait-for-expect": "^3.0.2"
  },
  "files": ["dist/**/*"],
  "ts-node": {
    "files": true
  }
}
