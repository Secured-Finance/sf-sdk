{
  "name": "@secured-finance/sf-client",
  "version": "0.2.0-beta.0",
  "description": "Javascript client library for the Secured Finance protocol",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "scripts": {
    "codegen": "ts-node ./scripts/generate.ts",
    "prebuild": "rm -rf dist",
    "watch": "tsc -p tsconfig.json -w",
    "build": "tsc --project tsconfig.json",
    "test": "ts-mocha src/*.test.ts src/**/*.test.ts",
    "test:cov": "nyc npm run test",
    "prepublish": "npm run codegen"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk/packages/sf-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/sf-sdk"
  },
  "dependencies": {
    "@secured-finance/sf-core": "^0.2.0-beta.0",
    "@secured-finance/smart-contracts": "^0.0.2-beta.3",
    "bignumber.js": "^9.0.2",
    "ethers": "^5.5.3",
    "gas-price-oracle": "^0.4.4",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/chai": "^4.3.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.21",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typechain": "^8.0.0",
    "web3-utils": "^1.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "types": "dist/index.d.ts",
  "ts-node": {
    "files": true
  }
}
