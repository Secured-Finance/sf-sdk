{
  "name": "@secured-finance/sf-client",
  "version": "0.2.0-beta.109",
  "description": "Javascript client library for the Secured Finance protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "codegen": "ts-node ./scripts/generate.ts && ts-node ./scripts/copy-deployment-files.ts",
    "postbuild": "cp -r ./src/deployments ./dist/deployments",
    "prebuild": "rm -rf dist",
    "watch": "tsc -p tsconfig.json -w",
    "build": "tsc --project tsconfig.json",
    "test": "jest --config jest.config.ts",
    "test:cov": "jest --config jest.config.ts --coverage",
    "prepublish": "npm run codegen"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk/packages/sf-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/sf-sdk"
  },
  "dependencies": {
    "@secured-finance/sf-core": "^0.2.0-beta.108",
    "@secured-finance/smart-contracts": "0.1.9-beta.1",
    "ethers": "^5.7.1",
    "gas-price-oracle": "^0.4.4"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/jest": "27.4.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.7.1",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.1.0",
    "typechain": "^8.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": ["dist"]
}
