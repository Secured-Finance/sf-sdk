{
  "name": "sf-sdk",
  "private": true,
  "description": "Monorepo of Javascript libraries for working with Secured Finance protocol",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "codegen": "nx run-many --target=codegen --all",
    "lint:check": "eslint './packages/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './packages/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json' '!lerna.json'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json' '!lerna.json'",
    "test-all": "nx run-many --target=test --all",
    "coverage": "nx run-many --target=test:cov --all",
    "build": "nx run-many --target=build --all",
    "prepublishOnly": "npm run build",
    "version": "npm run format:fix",
    "prepare": "husky install"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk#readme",
  "dependencies": {
    "lerna": "^4.0.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "dotenv": "^10.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "nx": "14.2.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ts-node": {
    "files": true
  }
}
