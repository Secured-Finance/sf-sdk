{
  "name": "sf-sdk",
  "private": true,
  "description": "Monorepo of Javascript libraries for working with Secured Finance protocol",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "lint": "eslint './packages/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './packages/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json' '!lerna.json'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}' '!package-lock.json' '!lerna.json'",
    "test-all": "lerna run test",
    "build": "lerna run build",
    "prepublishOnly": "npm run build",
    "version": "npm run format:fix",
    "prepare": "husky install"
  },
  "author": "Secured Finance",
  "bugs": {
    "url": "https://github.com/secured-finance/sf-sdk/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/secured-finance/sf-sdk#readme",
  "dependencies": {
    "lerna": "^4.0.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ts-node": {
    "files": true
  }
}
